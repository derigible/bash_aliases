### Git Command Bash Aliases

alias gpush='git push'
alias gpull='git pull'
alias greb='git rebase master'
alias gcom='git commit'
alias acom='git commit --amend'
alias gblame='git blame'
alias glog='git log'
alias gstat='git status'
alias gstag='git stage -u'
alias gdiff='git diff'
alias gchk='git checkout'
alias gbr='git branch'
alias grm='git reset HEAD'
alias gadd='git add'
alias gstash='git stash'
alias glp='git push origin $(git rev-parse --abbrev-ref HEAD)'
alias glfp='git push -f origin $(git rev-parse --abbrev-ref HEAD)'
